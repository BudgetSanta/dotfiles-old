# Ranger exit into current Dir
ranger-cd() {
    tempfile=$(mktemp /tmp/${tempfoo}.XXXXXX)
    /home/jared/.linuxbrew/bin/ranger --choosedir="$tempfile" "${@:-$(pwd)}" < $TTY
    test -f "$tempfile" &&
    if [ "$(cat -- "$tempfile")" != "$(echo -n `pwd`)" ]; then
        cd -- "$(cat "$tempfile")"
    fi
    rm -f -- "$tempfile"
}

# Make dir and change to it
mkcd() {
    mkdir "$1"
    cd "$1"
}

